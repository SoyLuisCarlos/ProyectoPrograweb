
@model IEnumerable<ProyectoPrograweb.Models.dbModels.NewsCategory>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NewsCategoryDescription)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NewsCategoryDescription)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdNewsCategory">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.IdNewsCategory">Detalles</a> |
                <a asp-action="Delete" asp-route-id="@item.IdNewsCategory">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
